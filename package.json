{
  "name": "@yecandir/hardhat-gui",
  "version": "0.0.1",
  "description": "Hardhat TypeScript plugin boilerplate",
  "repository": "github:nomiclabs/hardhat-ts-plugin-boilerplate",
  "author": "Nomic Labs LLC",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "ethereum",
    "smart-contracts",
    "hardhat",
    "hardhat-plugin"
  ],
  "scripts": {
    "postinstall": "cd ui && npm install",
    "lint:fix": "prettier --write 'src/**/*.{js,ts}' 'test/**/*.{js,ts}' && tslint --fix --config tslint.json --project tsconfig.json",
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "test": "mocha --exit --recursive 'test/**/*.test.ts'",
    "build": "tsc",
    "watch": "tsc -w",
    "prepublishOnly": "npm run build",
    "start": "cd ui && npm run start"
  },
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@semantic-release/github": "^8.0.6",
    "@types/chai": "^4.1.7",
    "@types/fs-extra": "^5.0.4",
    "@types/mocha": "^5.2.6",
    "@types/node": "^8.10.38",
    "chai": "^4.2.0",
    "hardhat": "^2.0.0",
    "mocha": "^7.1.2",
    "prettier": "2.0.5",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "hardhat": "^2.0.0"
  },
  "publishConfig": {
    "@yecandir:registry": "https://npm.pkg.github.com"
  }
}